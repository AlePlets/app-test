<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".utils.MyApplication"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Lillup"
        tools:targetApi="31">
        <activity
            android:name=".TesrActivity"
            android:exported="false" />
        <activity
            android:name=".passport.activities.PassportActivity"
            android:exported="false"
            >

        </activity>
        <activity
            android:name=".auth.activity.LoginActivity"
            android:launchMode="singleTop"
            android:exported="true"
            >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <!-- Accept URIs: {YOUR_APP_PACKAGE_NAME}://* -->
                <data
                    android:scheme="com.app.lillup"
                    android:host="auth" />
            </intent-filter>
        </activity>
        <activity
            android:name=".splash.MainActivity"
            android:exported="true"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".intro.activities.IntroScreenActivity"
            android:exported="true"
            >

        </activity>
    </application>
</manifest>